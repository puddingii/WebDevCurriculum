{ 
    "env": { 
        "browser": true, 
        "es6": true, 
        "node": true,
        "2021": true
    }, 
    "extends": "eslint:recommended", 
    "parserOptions": { 
        "ecmaVersion": 12, 
        "sourceType": "module" 
    }, 
    "rules": { 
        //"indent": ["error", "tab"], 
        "semi": ["error", "always"], 
        "keyword-spacing": 0, 
        "no-unused-vars": 1, 
        "space-before-function-paren": 0
    } 
}
